<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/myFitnessFragment">

    <fragment
        android:id="@+id/myFitnessFragment"
        android:name="com.continuouswave1550.fitness.fitnessprohome.myfitness.MyFitnessFragment"
        android:label="fragment_my_fitness"
        tools:layout="@layout/fragment_my_fitness" >
        <action
            android:id="@+id/action_myFitnessFragment_to_QRScanFragment"
            app:destination="@id/QRScanFragment" />
        <action
            android:id="@+id/action_myFitnessFragment_to_exerciseFragment"
            app:destination="@id/exerciseFragment" />
        <action
            android:id="@+id/action_myFitnessFragment_to_nutritionFragment"
            app:destination="@id/nutritionFragment" />
        <action
            android:id="@+id/action_myFitnessFragment_to_flexibilityFragment2"
            app:destination="@id/flexibilityFragment" />
    </fragment>
    <fragment
        android:id="@+id/QRScanFragment"
        android:name="com.continuouswave1550.fitness.fitnessprohome.qrscan.QRScanFragment"
        android:label="fragment_q_r_scan"
        tools:layout="@layout/fragment_q_r_scan" />
    <fragment
        android:id="@+id/aboutFragment"
        android:name="com.continuouswave1550.fitness.fitnessprohome.AboutFragment"
        android:label="fragment_about"
        tools:layout="@layout/fragment_about" />
    <fragment
        android:id="@+id/settingsFragment"
        android:name="com.continuouswave1550.fitness.fitnessprohome.SettingsFragment"
        android:label="fragment_settings"
        tools:layout="@layout/fragment_settings" />
    <fragment
        android:id="@+id/exerciseFragment"
        android:name="com.continuouswave1550.fitness.fitnessprohome.exercise.ExerciseFragment"
        android:label="fragment_exercise"
        tools:layout="@layout/fragment_exercise" />
    <fragment
        android:id="@+id/nutritionFragment"
        android:name="com.continuouswave1550.fitness.fitnessprohome.nutrition.NutritionFragment"
        android:label="fragment_nutrition"
        tools:layout="@layout/fragment_nutrition" />
    <fragment
        android:id="@+id/flexibilityFragment"
        android:name="com.continuouswave1550.fitness.fitnessprohome.flexibility.FlexibilityFragment"
        android:label="fragment_flexibility"
        tools:layout="@layout/fragment_flexibility" />
</navigation>